def vacuum_cleaner():
    rooms = {"A": 1, "B": 1}  # 1 = dirty, 0 = clean
    current_room = "A"
    steps = []

    while rooms["A"] == 1 or rooms["B"] == 1:
        if rooms[current_room] == 1:
            steps.append(f"Clean room {current_room}")
            rooms[current_room] = 0
        if current_room == "A":
            steps.append("Move to room B")
            current_room = "B"
        else:
            steps.append("Move to room A")
            current_room = "A"

    for step in steps:
        print(step)
    print("All rooms are clean")

vacuum_cleaner()


OUTPUT:
Clean room A
Move to room B
Clean room B
Move to room A
All rooms are clean
